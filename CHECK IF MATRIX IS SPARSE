def is_sparse(matrix):
    rows = len(matrix)
    cols = len(matrix[0])
    total_elements = rows * cols
    zero_count = 0

    for row in matrix:
        for elem in row:
            if elem == 0:
                zero_count += 1

    return zero_count > total_elements // 2
matrix = [
    [0, 0, 3],
    [0, 0, 0],
    [0, 1, 0]
]

if is_sparse(matrix):
    print("The matrix is sparse")
else:
    print("The matrix is not sparse")
